{
    "name": "matrozov/yii2-kit",
    "description": "Common classes for Yii2",
    "keywords": [
        "yii2",
        "yii2-extensions"
    ],
    "homepage": "https://github.com/matrozov/yii2-kit",
    "type": "extension",
    "license": "MIT",
    "version": "0.0.1",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Oleg Matrozov",
            "email": "oleg.matrozov@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/matrozov/yii2-kit/issues",
        "source": "https://github.com/matrozov/yii2-kit"
    },
    "require": {
        "thamtech/yii2-uuid": "^1.2",
        "league/flysystem": "^3.27",
        "yiisoft/yii2-httpclient": "^2.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "php": ">=8.2.0",
        "ext-imagick": "*",
        "yiisoft/yii2": "~2.0.45"
    },
    "autoload": {
        "psr-4": {
            "matrozov\\yii2kit\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
